@model Nop.Plugin.Misc.Nexport.Models.RegistrationField.NexportRegistrationFieldModel

@{
    ViewBag.Title = T("Admin.Customers.Nexport.RegistrationField.Fields.AddNew").Text;
    Html.SetActiveMenuItemSystemName("Customer and user settings");
}

<form asp-controller="NexportIntegration" asp-action="CreateRegistrationField" method="post" id="registrationfield-form">
    <div class="content-header clearfix">
        <h1 class="pull-left">
            @T("Admin.Customers.Nexport.RegistrationField.Fields.AddNew")
            <small>
                <i class="fa fa-arrow-circle-left"></i>
                <a asp-action="ListRegistrationField" asp-controller="NexportIntegration">
                    @T("Admin.Customers.Nexport.RegistrationField.Fields.BackToList")
                </a>
            </small>
        </h1>
        <div class="pull-right">
            <button type="submit" name="save" class="btn bg-blue">
                <i class="fa fa-floppy-o"></i>
                @T("Admin.Common.Save")
            </button>
            <button type="submit" name="save-continue" class="btn bg-blue">
                <i class="fa fa-floppy-o"></i>
                @T("Admin.Common.SaveContinue")
            </button>
        </div>
    </div>
    @await Html.PartialAsync("~/Plugins/Misc.Nexport/Areas/Admin/Views/RegistrationField/_CreateOrUpdate.cshtml", Model)
</form>